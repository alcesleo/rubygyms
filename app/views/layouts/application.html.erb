<!DOCTYPE html>
<html>
<head>
  <title>Rubygyms</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container master-container">
    <h1>Rubygyms</h1>

    <% flash.each do |type, message| %>
      <div class="<%= bootstrap_flash_class(type) %>"><%= message %></div>
    <% end %>

    <%= yield %>
  </div>
</body>
</html>
